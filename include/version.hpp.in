#ifndef LIBGOLF_VERSION_HPP
#define LIBGOLF_VERSION_HPP

#define GOLF_VERSION_MAJOR @GOLF_VERSION_MAJOR@
#define GOLF_VERSION_MINOR @GOLF_VERSION_MINOR@
#define GOLF_VERSION_PATCH @GOLF_VERSION_PATCH@
#define GOLF_VERSION_STRING "@GOLF_VERSION@"

namespace golf {
    struct Version {
        static constexpr int MAJOR = GOLF_VERSION_MAJOR;
        static constexpr int MINOR = GOLF_VERSION_MINOR;
        static constexpr int PATCH = GOLF_VERSION_PATCH;
        static constexpr const char* STRING = GOLF_VERSION_STRING;
    };
}

#endif // LIBGOLF_VERSION_HPP
